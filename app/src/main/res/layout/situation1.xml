<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="24sp"
        android:text="@string/lyla"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:id="@+id/lyla_text_view"
        android:layout_alignParentLeft="true" />

    <TextView android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="24sp"
        android:text="@string/me"
        android:layout_alignParentBottom="true"
        android:id="@+id/me_text_view"
        android:layout_toEndOf="@id/lyla_text_view"
        android:layout_toRightOf="@id/lyla_text_view" />

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="24sp"
        android:text="@string/natalie"
        android:id="@+id/natalie_text_view"
        android:layout_above="@id/lyla_text_view"
        />

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="24sp"
        android:text="@string/jennie"
        android:layout_alignParentBottom="true"
        android:id="@+id/jennie_text_view"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true" />

    <TextView
        android:id="@id/omoju_text_view"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="24sp"
        android:text="@string/omoju"
        android:layout_alignParentEnd="true"
        android:layout_above="@id/jennie_text_view"
        android:layout_alignParentRight="true" />

    <TextView
        android:id="@+id/amy_text_view"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="24sp"
        android:text="@string/amy"
        tools:ignore="RelativeOverlap"
        android:layout_above="@id/omoju_text_view"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true" />

    <TextView
        android:id="@+id/ben_text_view"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="24sp"
        android:text="@string/ben"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true"/>

    <TextView
        android:id="@+id/kunal_text_view"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="24sp"
        android:text="@string/kunal"
        tools:ignore="RelativeOverlap"

        android:layout_toStartOf="@id/ben_text_view"
        android:layout_alignParentTop="true"
        android:layout_toLeftOf="@id/ben_text_view" />

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="24sp"
        android:text="@string/kagure"
        android:id="@+id/kagure_text_view"
        android:layout_alignParentEnd="true"
        tools:ignore="RelativeOverlap"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_toEndOf="@id/ben_text_view"
        android:layout_toRightOf="@id/ben_text_view"/>

</RelativeLayout>